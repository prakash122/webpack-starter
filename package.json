{
  "name": "webpack-starter",
  "version": "0.1.0",
  "description": "Webpack configuration for the front end projects",
  "main": "app/index.js",
  "scripts": {
    "build": "webpack --mode=production --config=webpack.production.config.js",
    "dev-build": "webpack --mode=development --config=webpack.development.config.js",
    "start": "webpack-dev-server --mode=development --config=webpack.development.config.js"
  },
  "author": "Prakash Kumar <mail@prakash.io>",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
